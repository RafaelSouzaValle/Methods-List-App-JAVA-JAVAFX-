<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" 
	fx:controller="methods_list.Controller">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<HBox alignment="CENTER_RIGHT" spacing="10.0">
		<Label alignment="CENTER_RIGHT" prefWidth="100" text="Classe:" />
		<TextField onAction="#onSearch"  fx:id="txtClassPath" prefWidth="300" />
		<Button onAction="#onSearch" prefWidth="100" text="Search" />
	</HBox>
	<ListView fx:id="listViewClasses" prefHeight="300" prefWidth="400" />
</VBox>
